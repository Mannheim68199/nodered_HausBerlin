        "node-red-contrib-cpu": "~0.0.4",
        "node-red-contrib-cron-plus": "~2.1.0",
        "node-red-contrib-custom-ffmpeg-video": "~0.1.2",
        "node-red-contrib-ffmpeg": "~0.1.1",
        "node-red-contrib-fs": "~1.4.1",
        "node-red-contrib-modbus": "~5.43.0",
        "node-red-contrib-dialog": "file:node-red-contrib-dialog",
