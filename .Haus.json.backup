    {
        "id": "b1b21543297ea7ef",
        "type": "function",
        "z": "8303fc1be24bb0db",
        "name": "sql",
        "func": "let iTZOffset = global.get(\"TimezoneOffset\");\nlet sRange = flow.get(\"Range\");\nlet dDateFrom = new Date();\ndDateFrom.setHours(dDateFrom.getHours() + iTZOffset);\nlet sDatum = dDateFrom.toISOString().substring(0, 19).replace(\"T\", \" \").replace(\"Z\", \"\");\nlet sSql = \"select * from Geiger where DATUM >= '$DATUM' order by DATUM;\";\nswitch (sRange) {\n    case \"1h\":\n        dDateFrom.setHours(dDateFrom.getHours() - 1);\n        sSql = \"select concat(substring(DATUM, 1,16), ':30') AS 'DATUM', AVG(CPM) AS 'CPM', AVG(CPM5) AS 'CPM5', AVG(CPM15) AS 'CPM15', AVG(uSV) as 'uSv' from Geiger where DATUM >= '$DATUM' GROUP BY concat(substring(DATUM, 1,16), ':30') order by DATUM;\";\n        break;\n    case \"3h\":\n        dDateFrom.setHours(dDateFrom.getHours() - 3);\n        sSql = \"select substring(DATUM, 1,19) as 'DATUM', CPM, CPM5, CPM15, uSv from Geiger where DATUM >= '$DATUM' order by DATUM;\";\n        break;\n    case \"6h\":\n        dDateFrom.setHours(dDateFrom.getHours() - 6);\n        sSql = \"select substring(DATUM, 1,19) as 'DATUM', CPM, CPM5, CPM15, uSv from Geiger where DATUM >= '$DATUM' order by DATUM;\";\n        break;        \n    case \"1T\":\n        dDateFrom.setDate(dDateFrom.getDate() - 1);\n        sSql = \"select concat(substring(DATUM, 1,13), ':30:00') AS 'DATUM', AVG(CPM) AS 'CPM', AVG(CPM5) AS 'CPM5', AVG(CPM15) AS 'CPM15', AVG(uSV) as 'uSv' from Geiger where DATUM >= '$DATUM' GROUP BY concat(substring(DATUM, 1,13), ':30:00') order by DATUM;\";\n        break;\n    case \"1W\":\n        dDateFrom.setDate(dDateFrom.getDate() - 7);\n        sSql = \"select concat(substring(DATUM, 1,10), ' 12:00:00') AS 'DATUM', AVG(CPM) AS 'CPM', AVG(CPM5) AS 'CPM5', AVG(CPM15) AS 'CPM15', AVG(uSV) as 'uSv' from Geiger where DATUM >= '$DATUM' GROUP BY concat(substring(DATUM, 1,10), ' 12:00:00') order by DATUM;\";\n        break;\n    case \"1M\":\n        dDateFrom.setMonth(dDateFrom.getMonth() - 1);\n        sSql = \"select concat(substring(DATUM, 1,10), ' 12:00:00') AS 'DATUM', AVG(CPM) AS 'CPM', AVG(CPM5) AS 'CPM5', AVG(CPM15) AS 'CPM15', AVG(uSV) as 'uSv' from Geiger where DATUM >= '$DATUM' GROUP BY concat(substring(DATUM, 1,10), ' 12:00:00') order by DATUM;\";\n        break;\n    case \"1J\":\n        dDateFrom.setFullYear(dDateFrom.getFullYear() - 1);\n        sSql = \"select concat(substring(DATUM, 1,7), '15 12:00:00') AS 'DATUM', AVG(CPM) AS 'CPM', AVG(CPM5) AS 'CPM5', AVG(CPM15) AS 'CPM15', AVG(uSV) as 'uSv' from Geiger where DATUM >= '$DATUM' GROUP BY concat(substring(DATUM, 1,7), '15 12:00:00') order by DATUM;\";\n}\nmsg.sDateFrom = dDateFrom.toISOString().substring(0, 13).replace(\"T\", \" \");\nmsg.select = sSql.replace(\"$DATUM\", msg.sDateFrom);\nif (msg.payload === \"Range\") {\n    msg.topic = `select * from Geiger limit 1;`;\n} else {\n    msg.Datum = sDatum;\n    msg.CPM = msg.payload.CPM;\n    msg.CPM5 = msg.payload.CPM5;\n    msg.CPM15 = msg.payload.CPM15;\n    msg.uSv = msg.payload.uSv;\n    msg.counts = msg.payload.status.c_total;\n    msg.topic = \"insert into Geiger (DATUM, CPM, CPM5,CPM15,uSv,counts) values ('\";\n    msg.topic += sDatum + \"', \";\n    msg.topic += msg.payload.CPM + \", \" + msg.payload.CPM5 + \", \";\n    msg.topic += msg.payload.CPM15 + \", \" + msg.payload.uSv + \", \";\n    msg.topic += msg.payload.status.c_total + \");\";\n}\nmsg.topic += msg.select;\nreturn msg;",
