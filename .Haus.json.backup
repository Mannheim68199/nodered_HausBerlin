        "type": "function",
        "z": "b62ceea325f336f3",
        "g": "5dfdfc74855d5858",
        "name": "prepare",
        "func": "msg.Unit = global.get(\"GasUnit\");\nlet iTZOffset = global.get(\"TimezoneOffset\");\nlet oUmrechnungen = msg.payload[4];\nmsg.o2kWh = oUmrechnungen.find( oItm => (oItm.TYP === \"Gas_qm_kWh\") );\nlet i2kWh = msg.o2kWh ? msg.o2kWh.ParamZahl_1 * msg.o2kWh.ParamZahl_2 : 0\nmsg.o2Euro = oUmrechnungen.find(oItem => (oItem.TYP === \"Gas_kWh_€\") );\nlet i2Euro = msg.o2Euro ? i2kWh * msg.o2Euro.Arbeitspreis / 10 : 0\nmsg.i2kWh = i2kWh;\nmsg.i2Euro = i2Euro;\n// Tageswert\nlet dDatum = new Date(msg.payload[0][0].DATUM);\ndDatum.setHours(dDatum.getHours() + iTZOffset);\nmsg.TZoffset = iTZOffset;\nmsg.Datum = dDatum.toLocaleString('de-de');\n//msg.Datum = String(dDatum.getDate()).padStart(2, '0') + \".\" + String(dDatum.getMonth() + 1).padStart(2, '0') + \".\" + dDatum.getFullYear();\nmsg.Gaszaehler = (msg.payload[0][0].WERT/1000);\n// aktueller Monat\n// letzte 12 Monate\nlet msgMonat = {};\nmsgMonat.Unit = msg.Unit;\nlet fLastValue = 0;\nlet oDaten = {Bezug: {}, Bezug_Tag: {}};\nlet dDate = new Date();\nlet aktMonat = dDate.toISOString().substring(0, 7);\nlet aktTag = dDate.toISOString().substring(0, 10);\nmsg.payload[1].forEach(oItem => {\n    let sMonat = oItem.DATUM.substring(0,7);\n    let fValue = parseInt(10 * (oItem.WERT - fLastValue)) / 10000;\n    fLastValue = oItem.WERT;\n    if (!oDaten.Bezug[sMonat]) {\n        if (msg.Unit === \"kWh\") {\n            fValue = fValue * i2kWh;\n        }\n        if (msg.Unit === \"€\") {\n            let sTag = oItem.DATUM.substring(0,10);\n            msg.o2Euro = oUmrechnungen.find(o => ( ( o.TYP === \"Gas_kWh_€\") && (o.DATUM_VON.toISOString().substring(0, 7) <= sTag) && (!o.DATUM_BIS || sTag < o.DATUM_BIS.toISOString().substring(0, 10)) ) );\n            let i2Euro = msg.o2Euro ? i2kWh * msg.o2Euro.Arbeitspreis : 0\n            fValue = fValue * i2Euro + msg.o2Euro.Grundpreis;\n        }\n        oDaten.Bezug[sMonat] = fValue;\n    }\n});\n//node.warn(\"aktMonat / Bezug \" + aktMonat + \" => \" + oDaten.Bezug[aktMonat]);\nmsgMonat.payload = 0;\nif (oDaten.Bezug[aktMonat] ) {\n    msgMonat.payload = oDaten.Bezug[aktMonat].toFixed(1);\n}\nmsgMonat.sMonat = aktMonat;\n\nlet fLastValue10 = 0;\nmsg.payload[3].forEach((oItem, idx) => {\n    let sTag = oItem.DATUM.substring(0, 10);\n    let fValue10 = parseInt(10 * (oItem.WERT - fLastValue10)) / 10000;\n    fLastValue10 = oItem.WERT;\n    if (!oDaten[sTag]) {\n        if (msg.Unit === \"kWh\") {\n            fValue10 = fValue10 * i2kWh;\n        }\n        if (msg.Unit === \"€\") {\n            let sTag = oItem.DATUM.substring(0, 10);\n            msg.o2Euro = oUmrechnungen.find(o => ((o.TYP === \"Gas_kWh_€\") && (o.DATUM_VON.toISOString().substring(0, 7) <= sTag) && (!o.DATUM_BIS || sTag < o.DATUM_BIS.toISOString().substring(0, 10))));\n            let i2Euro = msg.o2Euro ? i2kWh * msg.o2Euro.Arbeitspreis : 0\n            fValue10 = fValue10 * i2Euro + msg.o2Euro.Grundpreis/30;\n        }\n        oDaten.Bezug_Tag[sTag] = fValue10;\n    }\n});\n\n// -------------------------------------------------------------------------------------------------\n\nlet aBarValues = [];\nlet aBarLabels = [];\nlet msgJahr = {};\nmsgJahr.Unit = msg.Unit\nmsgJahr.Summe = 0;\ndDate.setMonth(dDate.getMonth() - 13);\nmsgJahr.StartDate = dDate.toISOString().substring(0,7);\nmsgJahr.payload = \"\";\nfor (let i = 0; i < 14; i++) {\n    let sDatum = dDate.toISOString().substring(0, 7);\n    let fValue = oDaten.Bezug[sDatum] || 0;\n    aBarValues.push(fValue.toFixed(2));\n    aBarLabels.push(sDatum.substring(5,7));\n    dDate.setMonth(dDate.getMonth() + 1);\n    if ( i > 1 ) {\n        msgJahr.Summe += fValue;\n    }\n}\n//node.warn(\"Jahr \" + msgJahr.Summe);\nmsgJahr.Summe = msgJahr.Summe.toFixed(1);\naBarValues.shift();\naBarLabels.shift();\nmsgJahr.aBarValues = aBarValues;\nmsgJahr.aBarLabels = aBarLabels;\nmsgJahr.payload = [{\n    \"labels\": aBarLabels,\n    \"series\": [\"Verbrauch\"],\n    \"data\": [aBarValues]\n}];\n// letzten 2 Jahre\nlet aBarValues2 = [];\nlet aBarLabels2 = [];\nlet fLastValue2 = 0;\nlet msgAll = {};\nmsgAll.Unit = (msg.Unit === \"kWh\") ? \"MWh\" : msg.Unit;\nlet iFixedTo = (msg.Unit === \"kWh\") ? 2 : 1;\nmsgAll.payload = \"\";\nmsgAll.aDaten = msg.payload[2];\nmsg.payload[2].forEach(oItem => {\n    let sDatum = oItem.DATUM;\n    let fValue = parseInt(10 * (oItem.WERT - fLastValue2)) / 10000;\n    fLastValue2 = oItem.WERT;\n    if ( sDatum === \"2021\" ) {\n        fValue += 1542.6;  // Verbrauch kWh von 2. Heizung !!\n    }\n    if (msg.Unit === \"kWh\") {\n        fValue = (fValue * i2kWh / 1000);\n    }\n    if (msg.Unit === \"€\") {\n        let sTag = oItem.DATUM.substring(0, 10);\n        msg.o2Euro = oUmrechnungen.find(o => ((o.TYP === \"Gas_kWh_€\") && (o.DATUM_VON.toISOString().substring(0, 7) <= sTag) && (!o.DATUM_BIS || sTag < o.DATUM_BIS.toISOString().substring(0, 10))));\n        let i2Euro = msg.o2Euro ? i2kWh * msg.o2Euro.Arbeitspreis : 0\n        fValue = fValue * i2Euro + msg.o2Euro.Grundpreis*12;\n        node.warn(\"Datum: \" + sDatum + \" Tag: \" + sTag + \" i2€: \" + i2Euro + \" Wert: \" + fValue);\n    }\n//    node.warn(\"Datum / Wert \" + +sDatum + \" / \" + fValue);\n    aBarValues2.push(fValue.toFixed(iFixedTo));\n    aBarLabels2.push(sDatum);\n});\naBarValues2.shift();\naBarLabels2.shift();\nmsgAll.aBarValues = aBarValues2;\nmsgAll.aBarLabels = aBarLabels2;\nmsgAll.payload = [{\n    \"labels\": aBarLabels2,\n    \"series\": [\"Verbrauch\"],\n    \"data\": [aBarValues2]\n}];\n\n// letzten 10 Tage\nlet aBarValues10 = [];\nlet aBarLabels10 = [];\nlet msg10Tage = {};\nmsg10Tage.Unit = msg.Unit;\nmsg10Tage.Summe = 0;\nlet o10Tage = {};\nmsg10Tage.dDateBis = new Date();\nmsg10Tage.dDateVon = new Date();\nmsg10Tage.dDateVon = new Date(msg10Tage.dDateVon.setDate(msg10Tage.dDateVon.getDate() - 10));\nlet dDate10 = msg10Tage.dDateVon;\n//node.warn(\"Tag => Bezug_Tag \" + aktTag + \"=>\" + oDaten.Bezug_Tag[aktTag]);\nmsg10Tage.payload = 0;\nif ( oDaten.Bezug_Tag[aktTag] ) {\n    msg10Tage.payload = oDaten.Bezug_Tag[aktTag].toFixed(1);\n}\nmsg10Tage.sTag = aktTag;\nmsg10Tage.aData = msg.payload[3];\nmsg10Tage.StartDate = dDate10.toISOString().substring(0, 10);\nmsg10Tage.payload = \"\";\nfor (let i = 0; i < 11; i++) {\n    let sDatum = dDate10.toISOString().substring(0, 10);\n    //node.warn(\"Datum / 10TageWert \" + sDatum + \" / \" + oDaten.Bezug_Tag[sDatum]);\n    let fValue = 0;\n    if ( oDaten.Bezug_Tag[sDatum] ) {\n        fValue = oDaten.Bezug_Tag[sDatum];\n        aBarValues10.push(fValue.toFixed(2));\n    } else {\n        //node.warn(\"10Tage: \" + sDatum + \" => \" + fValue);\n        aBarValues10.push(0);\n    }\n    aBarLabels10.push(sDatum.substring(8, 10));\n    dDate10.setDate(dDate10.getDate() + 1);\n    if (i > 0) {\n        msg10Tage.Summe += fValue;\n    }\n}\n/*\nmsg.payload[3].forEach( (oItem, idx) => {\n    let sTag = oItem.DATUM.substring(0, 10);\n    let fValue = parseInt(10 * (oItem.WERT - fLastValue10)) / 10000;\n    fLastValue10 = oItem.WERT;\n    if (!oDaten[sTag]) {\n        if (msg.Unit === \"kWh\") {\n            fValue = fValue * i2kWh;\n        }\n        if (msg.Unit === \"€\") {\n            fValue = fValue * i2Euro;\n        }\n        o10Tage[sTag] = fValue;\n        aBarValues10.push(fValue.toFixed(2));\n        aBarLabels10.push(sTag.substring(8, 10));\n        if (idx > 0) {\n            msg10Tage.Summe += fValue;\n        }\n    }\n});\n*/\n//node.warn( \"Summe = \" + msg10Tage.Summe);\nif (msg10Tage.Summe > 0 ) {\n    msg10Tage.Summe = msg10Tage.Summe.toFixed(1);\n} else {\n    msg10Tage.Summe = 0;\n}\naBarValues10.shift();\naBarLabels10.shift();\nmsg10Tage.aBarValues = aBarValues10;\nmsg10Tage.aBarLabels = aBarLabels10;\nmsg10Tage.payload = [{\n    \"labels\": aBarLabels10,\n    \"series\": [\"Verbrauch\"],\n    \"data\": [aBarValues10]\n}];\n\nreturn [ msg, msgMonat, msgJahr, msgAll, msg10Tage ];",
