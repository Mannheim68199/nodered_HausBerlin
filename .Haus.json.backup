        "func": "let sZeitraum = \"DAYS\";\nswitch ( flow.get(\"Zeitraum\") ) {\n    case \"T\":\n        sZeitraum = \"DAY\";\n        break;\n    case \"h\":\n        sZeitraum = \"HOUR\";\n        break;\n    case \"M\":\n        sZeitraum = \"MONTH\";\n}\nlet iRange = parseInt(flow.get(\"Range\")) || 1;\n//node.warn(\"Zeitraum: \" + sZeitraum + \" Range: \" + iRange);\nmsg.topic = \"select CONCAT(SUBSTRING(DATETIME, 1, 15),'5:00') as 'DATUM', AVG(Temperatur) as 'Temperatur', AVG(Luftdruck) as 'Luftdruck', AVG(Luftfeuchtigkeit) as 'Luftfeuchtigkeit' from Feinstaub where DATETIME >= DATE_ADD(NOW(), INTERVAL -\" + iRange + \" \" + sZeitraum + \" ) GROUP BY SUBSTRING(DATUM, 1, 15) order by DATETIME asc;\";\nmsg.topic += \"select CONCAT(SUBSTRING(DATUM, 1, 15),'5:00') AS 'DATUM', AVG(Temperatur) as 'Temperatur', AVG(Luftdruck) as 'Luftdruck', AVG(Luftfeuchtigkeit) as 'Luftfeuchtigkeit', AVG(Spannung) as 'Spannung' from Sensor1 where DATUM >= DATE_ADD(NOW(), INTERVAL -\" + iRange + \" \" + sZeitraum + \") GROUP BY SUBSTRING(DATUM, 1, 15) order by DATUM asc;\";\nmsg.topic += \"select CONCAT(SUBSTRING(DATUM, 1, 15),'5:00') AS 'DATUM', AVG(Temperatur) as 'Temperatur', AVG(Luftdruck) as 'Luftdruck', AVG(Luftfeuchtigkeit) as 'Luftfeuchtigkeit', AVG(RSSI) as 'RSSI', AVG(Spannung) as 'Spannung' from Sensor2 where DATUM >= DATE_ADD(NOW(), INTERVAL -\" + iRange + \" \" + sZeitraum + \") GROUP BY SUBSTRING(DATUM, 1, 15) order by DATUM asc;\";\nmsg.topic += \"select CONCAT(SUBSTRING(DATUM, 1, 15),'5:00') AS 'DATUM', AVG(Temperatur) as 'Temperatur', AVG(Luftdruck) as 'Luftdruck', AVG(Luftfeuchtigkeit) as 'Luftfeuchtigkeit', AVG(RSSI) as 'RSSI', AVG(Spannung) as 'Spannung' from Sensor3 where DATUM >= DATE_ADD(NOW(), INTERVAL -\" + iRange + \" \" + sZeitraum + \") GROUP BY SUBSTRING(DATUM, 1, 15) order by DATUM asc;\";\nmsg.topic += \"select CONCAT(SUBSTRING(DATUM, 1, 15),'5:00') AS 'DATUM', AVG(Temperatur) as 'Temperatur', AVG(Luftdruck) as 'Luftdruck', AVG(Luftfeuchtigkeit) as 'Luftfeuchtigkeit', AVG(RSSI) as 'RSSI', AVG(Spannung) as 'Spannung' from Sensor4 where DATUM >= DATE_ADD(NOW(), INTERVAL -\" + iRange + \" \" + sZeitraum + \") GROUP BY SUBSTRING(DATUM, 1, 15) order by DATUM asc;\";\nreturn msg;",
