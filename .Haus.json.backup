    {
        "id": "bd700920a191af75",
        "type": "function",
        "z": "1e35c796161c7d26",
        "name": "",
        "func": "msg.topic = [ \"Post\" ];\nlet aData = [];\nlet oWerte = {};\nlet aLabel = [];\nlet avWert = 0;\nlet iRange = 10;\nlet iLastWert = 0;\nlet iLen = msg.payload.length;\nif ( iLen > 200 ) {\n    let iDiv = parseInt(Math.log10(iLen)) - 1;\n    iRange = parseInt(iLen / (100*(parseInt(Math.log10(iLen))-1)) ) ;\n} else {\n    iRange = 2;\n}\nmsg.iLen = iLen;\nmsg.iDiv = parseInt(Math.log10(iLen)) - 1;\nmsg.iRange = iRange;\nmsg.payload.forEach( (oItem) => {\n    let sMinutes = oItem.DATUM.toISOString().substring(0, 16).replace(\"T\", \" \");\n    let sHours = oItem.DATUM.toISOString().substring(0, 13).replace(\"T\", \" \");\n    //node.warn(\"sMinutes: \" + sMinutes + \" Wert: \" + oItem.WERT);\n    if ( !oWerte[sHours] ) {\n        oWerte[sHours] = [];\n    }\n    oWerte[sHours].push({\n        \"Time\": sMinutes,\n        \"Value\": oItem.WERT\n    })\n});\nlet sFromDate = flow.get(\"fromDate\");\nmsg.dDatum = new Date(sFromDate);\nmsg.dDatumBis = new Date();\nlet sTime = \"\";\nlet aDatum = [];\nlet dDatum = new Date(msg.dDatum);\nwhile ( dDatum <= msg.dDatumBis) {\n    sTime = dDatum.toISOString().substring(0, 13).replace(\"T\", \" \");\n    aDatum.push(sTime);\n    let aValues = oWerte[sTime] ? oWerte[sTime]: [];\n    if ( aValues.length > 0 ) {\n        aValues.forEach( oValue => {\n            aLabel.push(oValue.Time);\n            aData.push(oValue.Value);\n            iLastWert = oValue.Value;\n        })\n    } else {\n        aLabel.push(sTime);\n        aData.push(iLastWert);\n    }\n    dDatum = new Date(dDatum.setHours(dDatum.getHours() + 1));\n}\n\n\nlet min = Math.min(...aData);\nlet max = Math.max(...aData) || 100;\nmsg.min = min;\nmsg.max = max;\nmax = (max < 0) ? 50 : max;\nmin = (min < 0) ? 0 : min;\nlet nMaxBase = parseInt(Math.log10(max));\nmsg.maxBaseNumber = Math.pow(10, nMaxBase);\nlet nMax = 1.5 * parseInt(max / msg.maxBaseNumber) * msg.maxBaseNumber;\nnMax = nMax < 40 ? 80 : nMax;\nlet nMin = 0; //nMax > 50 ? 0 : -nMax;\nmsg.nMax = nMax;\nmsg.nMin = nMin;\nmsg.nMaxBase = nMaxBase;\nmsg.aData = msg.payload;\nmsg.oWerte = oWerte;\nmsg.ui_control = {};\nmsg.ui_control.ymax = nMax;\nmsg.ui_control.ymin = nMin;\nmsg.payload = [{ \n    \"labels\": aLabel, \n    \"series\": [\"Post\"], \n    \"data\": [aData]\n}];\nreturn msg;\n",
