        "name": "getSql",
        "func": "let dDate = new Date();\ndDate.setDate(dDate.getDate()-1);\nlet sDatumAb = dDate.toISOString().substring(0, 19).replace(\"T\",\" \");\nmsg.Quelle = msg.topic;\nswitch (msg.Quelle) {\n    case 'Haus':\n        msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `STROM_AKT_VERBRAUCH` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n        msg.Typ = \"Verbrauch\";\n        break;\n    case 'Netz':\n        let bFlagNetz = !!global.get(\"Netz_Flag\");\n        if ( bFlagNetz ) {\n            msg.topic =  \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `STROM_AKT_LIEFERUNG` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n            msg.Typ = \"Lieferung Wh\";\n        } else {\n            msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `STROM_AKT_BEZUG` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n            msg.Typ = \"Bezug Wh\";\n        }\n        global.set(\"Netz_Flag\", !bFlagNetz);\n        break;\n    case 'PV':\n        msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `PV_AKT_LEISTUNG` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n        msg.Typ = \"Erzeugung Wh\";\n        break;\n    case 'BKW':\n        msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(Power) as 'Verbrauch' FROM `BKL` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n        msg.Typ = \"Erzeugung Wh\";\n        break;\n    case 'Batterie':\n        let iBat = parseInt(global.get(\"Batterie_enum\")) ? global.get(\"Batterie_enum\") : 0;\n        //node.warn(\" Batterie: \" + iBat);\n        switch ( iBat ) {\n            case 0:\n                msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `BATTERIE_SoC` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n                msg.Typ = \"SoC %\";\n                break;\n            case 1:\n                msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `BATTERIE_ENTLADUNG_10MinutenWerte_V` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n                msg.Typ = \"Entladung Wh\";\n                break;\n            case 2:\n                msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `BATTERIE_LADUNG_10MinutenWerte_V` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n                msg.Typ = \"Ladung Wh\";\n                break;\n            default:\n                msg.topic = \" SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(WERT) as 'Verbrauch' FROM `BATTERIE_SoC` where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY `DATUM` DESC; \";\n                msg.Typ = \"SoC %\";\n        }\n        iBat = (iBat + 1) % 3;\n        global.set(\"Batterie_enum\", iBat);\n        break;\n    case 'Auto':\n        msg.topic = \"SELECT SUBSTRING(DATUM, 1, 15) AS 'Datum', AVG(Energie) as 'Verbrauch' FROM ECAR where DATUM > '\" + sDatumAb + \"' GROUP BY SUBSTRING(DATUM, 1, 15) ORDER BY DATUM desc;\";\n        msg.Typ = \"Verbrauch Wh\";\n        break;\n    default:\n        return;\n}\nreturn msg;\n",
