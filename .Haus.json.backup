    {
        "id": "66ebaba1807a2c45",
        "type": "function",
        "z": "89e19cdc5c0c0281",
        "g": "69bbe98a199311b8",
        "name": "prepareChart",
        "func": "let aBarValues = [];\nlet aBarLabels = [];\nmsg.aDaten = msg.payload;\nlet dDate = msg.fromDate;\nlet maxValue = 0;\nlet iTZOffset = 0; //global.get(\"TimezoneOffset\") || 2;\nfor (let i = 0; i <= msg.iRange; i++) {\n    let fValue = 0;\n    let sDatum = dDate.toISOString().substring(0, msg.len).replace(\"T\", \" \");\n    //node.warn(\" Datum: \" + sDatum);\n    let oItem = msg.payload.find(oItm => {\n        return (oItm.DATUM === sDatum);\n    });\n    if (oItem) {\n        //node.warn(\" Wert: \" + oItem.Anzahl + \" altWert: \" + maxValue );\n    }\n    if (oItem) {\n        fValue = oItem.Anzahl * 0.21;\n        maxValue = maxValue > fValue ? maxValue : fValue;\n    }\n    if (msg.sRange === \"hour\") {\n        sDatum = sDatum.slice(-2);\n    }\n    aBarValues.push(fValue.toFixed(2));\n    let dDateShow = new Date(dDate);\n    dDateShow.setHours(dDate.getHours() + iTZOffset);\n    let sDatumShow = dDateShow.toISOString().substring(0, msg.len).replace(\"T\", \" \");\n\n    switch (msg.sRange) {\n        case 'hour':\n            dDate.setHours(dDate.getHours() + 1);\n            aBarLabels.push(sDatumShow.substring(11, 13) + \":30\");\n            break;\n        case 'day':\n            dDate.setDate(dDate.getDate() + 1);\n            aBarLabels.push(sDatumShow.substring(8, 10));\n            break;\n        case 'month':\n            dDate.setMonth(dDate.getMonth() + 1);\n            aBarLabels.push(sDatumShow.substring(5, 7));\n            break;\n        case 'year':\n            dDate.setFullYear(dDate.getFullYear() + 1);\n            aBarLabels.push(sDatumShow.substring(0, 4));\n    }\n}\naBarValues.shift();\naBarLabels.shift();\nmsg.aBarValues = aBarValues;\nmsg.aBarLabels = aBarLabels;\n\nlet iStep = 4;\nmaxValue = maxValue < iStep ? iStep : parseInt((maxValue + iStep) / iStep) * iStep;\nmsg.ui_control = {};\nmsg.ui_control.ymax = maxValue;\nmsg.ui_control.ymin = 0;\n\nmsg.payload = [{\n    \"labels\": aBarLabels,\n    \"series\": [\"Regen3\"],\n    \"data\": [aBarValues]\n}];\nreturn msg;",
