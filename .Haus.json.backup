        "func": "let sSql = `select DATUM, WERT from SMA_STROM_BEZUG order by DATUM desc limit 1; \nselect * from Parameter where TYP like 'Strom%' order by DATUM_VON desc;\nSELECT SUBSTRING(DATUM,1, 4) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM >= DATE_ADD(NOW(), INTERVAL -6 YEAR) group by SUBSTRING(DATUM,1,4) order by SUBSTRING(DATUM, 1, 4) asc;\nSELECT SUBSTRING(DATUM,1, 7) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM >= DATE_ADD(NOW(), INTERVAL -14 MONTH) group by SUBSTRING(DATUM,1,7);\nSELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM >= DATE_ADD(NOW(), INTERVAL -11 DAY) group by SUBSTRING(DATUM,1,10) union (SELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM < DATE_ADD(NOW(), INTERVAL -11 DAY) LIMIT 1 ) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1, 4) as 'DATUM', MAX(Energie) as 'WERT' FROM ECAR where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4) order by SUBSTRING(DATUM, 1, 4) asc;\nSELECT SUBSTRING(DATUM,1, 7) as 'DATUM', MAX(ENERGIE) as 'WERT' FROM ECAR where DATUM >= DATE_ADD(NOW(), INTERVAL -14 MONTH) group by SUBSTRING(DATUM,1,7) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(ENERGIE) as 'WERT' FROM ECAR where DATUM >= DATE_ADD(NOW(), INTERVAL -11 DAY) group by SUBSTRING(DATUM,1,10) union (SELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(ENERGIE) as 'WERT' FROM ECAR where DATUM < DATE_ADD(NOW(), INTERVAL -11 DAY) LIMIT 1 ) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1, 4) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4) order by SUBSTRING(DATUM, 1, 4) asc;\nSELECT SUBSTRING(DATUM,1, 7) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM >= DATE_ADD(NOW(), INTERVAL -14 MONTH) group by SUBSTRING(DATUM,1,7) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM >= DATE_ADD(NOW(), INTERVAL -11 DAY) group by SUBSTRING(DATUM,1,10) union (SELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM < DATE_ADD(NOW(), INTERVAL -11 DAY) LIMIT 1 ) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1, 4) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\nSELECT SUBSTRING(DATUM,1, 7) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\nSELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM,1,10) union (SELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1 ) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1, 4) as 'DATUM', MAX(TotalEnergy)/1000 as 'WERT' FROM BKW where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\nSELECT SUBSTRING(DATUM,1, 7) as 'DATUM', MAX(TotalEnergy)/1000 as 'WERT' FROM BKW where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\nSELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(TotalEnergy)/1000 as 'WERT' FROM BKW where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM,1,10) union (SELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(TotalEnergy)/1000 as 'WERT' FROM BKW where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1 ) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1, 4) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_LADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\nSELECT SUBSTRING(DATUM,1, 7) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_LADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\nSELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_LADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM,1,10) union (SELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_LADUNG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1 ) order by DATUM asc;\nSELECT SUBSTRING(DATUM,1, 4) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\nSELECT SUBSTRING(DATUM,1, 7) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\nSELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM,1,10) union (SELECT SUBSTRING(DATUM,1,10) as 'DATUM', MAX(WERT)/1000 as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1 ) order by DATUM asc;\n`;\nmsg.topic = sSql;\nreturn msg;3",
