    {
        "id": "0e64811718208d8d",
        "type": "function",
        "z": "89e19cdc5c0c0281",
        "g": "69bbe98a199311b8",
        "name": "getSql",
        "func": "let now = new Date();\nlet sRange = flow.get(\"Range\") || \"hour\";\nif (sRange === \"hour\") {\n    msg.fromDate = new Date(now.setHours(now.getHours() - 22));\n    msg.iRange = 24;\n    msg.len = 13;\n    msg.label = \"[l/h]  1 Tag \";\n    msg.topic = `SELECT SUBSTRING(DATUM,1,13) as 'DATUM', SUM(Wert) as 'Anzahl' FROM Regen3 where DATUM >= DATE_ADD(NOW(), INTERVAL -2 DAY) group by SUBSTRING(DATUM,1,13);`;\n} else if (sRange === \"day\") {\n    msg.fromDate = new Date(now.setDate(now.getDate() - 7));\n    msg.iRange = 7;\n    msg.len = 10;\n    msg.label = \"[l/Tag]  1 Woche \";\n    msg.topic = `SELECT SUBSTRING(DATUM,1,10) as 'DATUM', SUM(Wert) as 'Anzahl' FROM Regen3 where DATUM >= DATE_ADD(NOW(), INTERVAL -14 DAY) group by SUBSTRING(DATUM,1,10);`;\n} else if (sRange === \"month\") {\n    msg.fromDate = new Date(now.setMonth(now.getMonth() - 12));\n    msg.iRange = 12;\n    msg.len = 7;\n    msg.label = \"[l/Monat]  1 Jahr \";\n    msg.topic = `SELECT SUBSTRING(DATUM,1,7) as 'DATUM', SUM(Wert) as 'Anzahl' FROM Regen3 where DATUM >= DATE_ADD(NOW(), INTERVAL -1 YEAR) group by SUBSTRING(DATUM,1,7);`;\n} else if (sRange === \"year\") {\n    msg.fromDate = new Date(now.setFullYear(now.getFullYear() - 5));\n    msg.iRange = 5;\n    msg.len = 4;\n    msg.label = \"[l/Jahr]  5 Jahre \";\n    msg.topic = `SELECT SUBSTRING(DATUM,1,4) as 'DATUM', SUM(Wert) as 'Anzahl' FROM Regen3 group by SUBSTRING(DATUM,1,4);`;\n}\nmsg.sRange = sRange;\nreturn msg;",
