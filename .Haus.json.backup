        "z": "b62ceea325f336f3",
        "g": "066010207daa49fb",
        "name": "prepare",
        "func": "msg.topic = \"\";\nlet fLastValue = 0;\nlet dDate = new Date();\nmsg.Datum = dDate.toLocaleString('de-de');\nmsg.Wasserzaehler = (msg.payload[0][0].WERT);\nlet msgJahre = {};\nlet msgMonate = {};\nlet msgTage = {};\nlet oRange = { Jahr: { len: 4, cnt: 6, von: 0, bis: 4} , Monat: { len: 7, cnt: 13, von: 5, bis: 7}, Tag: {len: 10, cnt: 10, von: 8, bis: 10} };\nlet oKat = { Bezug: { show: true }, Auto: { show: true } };\nlet aMap = [ {idx:2,  Kat: \"Bezug\", Typ: \"Tag\"}, {idx:3,  Kat: \"Bezug\", Typ: \"Monat\"}, {idx:4,  Kat: \"Bezug\", Typ: \"Jahr\"} ];\nlet oDaten = {};\naMap.forEach( oItm => {\n    let idx = oItm.idx;\n    let sTyp = oItm.Typ;\n    let sKat = oItm.Kat;\n    if ( !oDaten[sTyp] ) {\n        oDaten[sTyp] = {};\n    }\n    if ( !oDaten[sTyp][sKat]) {\n        oDaten[sTyp][sKat] = {}\n    }\n    // node.warn(\" idx: \" + idx + \" sTyp: \" + sTyp + \" Kat: \" + sKat);\n    fLastValue = 0;\n    msg.payload[idx].forEach(oItem => {\n        let iLen = oRange[sTyp].len;\n        let sDate = oItem.DATUM.substring(0,iLen);\n        let fValue = parseInt(10 * (oItem.WERT - fLastValue)) / 10;\n        fLastValue = oItem.WERT;\n        if (!oDaten[sTyp][sKat][sDate]) {\n            oDaten[sTyp][sKat][sDate] = fValue;\n        }\n    });\n});\nmsg.oDaten = oDaten;\nlet oBarValues = {};\nlet oBarLabels = {};\nlet oBarSeries = {};\nlet oBarData =   {};\n\nObject.keys(oRange).forEach( sRange => {\n    let iLen = oRange[sRange].len;\n    let iCnt = oRange[sRange].cnt;\n    let iVon = oRange[sRange].von;\n    let iBis = oRange[sRange].bis;\n\n    let dDate = new Date();\n    dDate.setHours(dDate.getHours() + 6);\n\n    //node.warn(\"Range: \" + sRange + \" Cnt: \" + iCnt + \" Date: \" + dDate.toISOString().substring(0, 10) );\n\n    switch (sRange) {\n        case \"Jahr\":\n            dDate.setFullYear(dDate.getFullYear() - iCnt + 1);\n            break;\n        case \"Monat\":\n            dDate.setMonth(dDate.getMonth() - iCnt + 1);\n            break;\n        case \"Tag\":\n            dDate.setDate(dDate.getDate() - iCnt + 1);\n    }\n    //node.warn(\"Range: \" + sRange + \" Date: \" + dDate.toISOString().substring(0, 10));\n\n    for (let i = 1; i <= iCnt; i++) {\n        let sDate = dDate.toISOString().substring(0, iLen);\n        //node.warn(\"Range: \" + sRange + \" Date: \" + sDate + \" Datum: \" + dDate.toISOString().substring(0, 10));\n        Object.keys(oDaten[sRange]).forEach( sKat => {\n            if (!oBarValues[sRange]) {\n                oBarValues[sRange] = {};\n            }\n            if (!oBarValues[sRange][sKat]) {\n                oBarValues[sRange][sKat] = [];\n            }\n            if (!oBarData[sRange]) {\n                oBarData[sRange] = [];\n            }\n            if (!oBarLabels[sRange]) {\n                oBarLabels[sRange] = [];\n            }\n            if (!oBarSeries[sRange]) {\n                oBarSeries[sRange] = [];\n            }\n            let fValue = oDaten[sRange][sKat][sDate] ? oDaten[sRange][sKat][sDate] : 0;\n            oBarValues[sRange][sKat].push(fValue.toFixed(2));\n            if (i === iCnt && oKat[sKat].show ) {\n                oBarSeries[sRange].push(sKat);\n                oBarData[sRange].push(oBarValues[sRange][sKat]);\n            }\n        })\n        oBarLabels[sRange].push(sDate.substring(iVon, iBis));\n//        if ( sRange === \"Monat\" ) {\n//            node.warn(\"Range: \" + sRange + \" Date: \" + dDate.toISOString().substring(0,10) + \" Label: \" + sDate.substring(iVon, iBis));\n//        }\n        switch (sRange) {\n            case \"Jahr\":\n                dDate.setFullYear(dDate.getFullYear() + 1 );\n                break;\n            case \"Monat\":\n                dDate.setMonth(dDate.getMonth() + 1 );\n                break;\n            case \"Tag\":\n                dDate.setDate(dDate.getDate()   + 1 );\n        }\n    }\n\n    switch (sRange) {\n        case \"Jahr\":\n            msgJahre.payload = [{\n                \"labels\": oBarLabels[sRange],\n                \"series\": oBarSeries[sRange],\n                \"data\": oBarData[sRange]\n            }];\n            break;\n        case \"Monat\":\n            msgMonate.payload = [{\n                \"labels\": oBarLabels[sRange],\n                \"series\": oBarSeries[sRange],\n                \"data\": oBarData[sRange]\n            }];\n            break;\n        case \"Tag\":\n            msgTage.payload = [{\n                \"labels\": oBarLabels[sRange],\n                \"series\": oBarSeries[sRange],\n                \"data\": oBarData[sRange]\n            }];\n    }\n});\n\nmsg.oBarValues = oBarValues;\nmsg.oBarLabels = oBarLabels;\nmsg.oBarSeries = oBarSeries;\nmsg.oBarData   = oBarData;\nreturn [msg, msgTage, msgMonate, msgJahre];\n\n/*\n\n\n\n\nlet oUmrechnungen = msg.payload[1];\nlet dDatum = new Date();\ndDatum.setHours(dDatum.getHours()+6);\nmsg.Datum = dDatum.toLocaleString('de-de');\nmsg.Wasserzaehler = (msg.payload[0][0].WERT);\nlet oDaten = { Bezug_Jahr: {}, Bezug_Monat: {}, Bezug_Tag: {} };\nlet fLastValue = 0;\nlet dDate = new Date();\nlet aktMonat = dDate.toISOString().substring(0, 7);\nlet aktTag = dDate.toISOString().substring(0, 10);\n\n// letzten 10 Tage\nmsg.payload[2].forEach((oItem, idx) => {\n    let sTag = oItem.DATUM.substring(0, 10);\n    let fValue = parseInt(10 * (oItem.WERT - fLastValue)) / 10;\n    fLastValue = oItem.WERT;\n    if (!oDaten[sTag]) {\n        oDaten.Bezug_Tag[sTag] = fValue;\n    }\n});\n\n// letzte 12 Monate\nlet msgMonat = {};\nmsgMonat.payload = 0;\nfLastValue = 0;\nmsg.payload[3].forEach(oItem => {\n    let sMonat = oItem.DATUM.substring(0,7);\n    let fValue = parseInt(10 * (oItem.WERT - fLastValue)) / 10000;\n    fLastValue = oItem.WERT;\n    if (!oDaten.Bezug_Monat[sMonat]) {\n        oDaten.Bezug_Monat[sMonat] = fValue;\n    }\n});\n//node.warn(\"aktMonat / Bezug \" + aktMonat + \" => \" + oDaten.Bezug[aktMonat]);\n\n// letzten 5 Jahre\nlet msgJahr = {};\nmsgJahr.payload = 0;\nfLastValue = 0;\nmsg.payload[4].forEach(oItem => {\n    let sJahr = oItem.DATUM.substring(0,4);\n    let fValue = parseInt(10 * (oItem.WERT - fLastValue)) / 10000;\n    fLastValue = oItem.WERT;\n    if (!oDaten.Bezug_Jahr[sJahr]) {\n        oDaten.Bezug_Jahr[sJahr] = fValue;\n    }\n});\n\nmsg.oDaten = oDaten;\n\n// -------------------------------------------------------------------------------------------------\n// letzten 10 Tage\nlet aBarValues10 = [];\nlet aBarLabels10 = [];\nlet msg10Tage = {};\nmsg10Tage.Unit = msg.Unit;\nmsg10Tage.Summe = 0;\nlet o10Tage = {};\nmsg10Tage.dDateBis = new Date();\nmsg10Tage.dDateVon = new Date();\nmsg10Tage.dDateVon = new Date(msg10Tage.dDateVon.setDate(msg10Tage.dDateVon.getDate() - 10));\nlet dDate10 = msg10Tage.dDateVon;\n//node.warn(\"Tag => Bezug_Tag \" + aktTag + \"=>\" + oDaten.Bezug_Tag[aktTag]);\nmsg10Tage.payload = 0;\nif ( oDaten.Bezug_Tag[aktTag] ) {\n    msg10Tage.payload = oDaten.Bezug_Tag[aktTag].toFixed(1);\n}\nmsg10Tage.sTag = aktTag;\nmsg10Tage.aData = msg.payload[3];\nmsg10Tage.StartDate = dDate10.toISOString().substring(0, 10);\nmsg10Tage.payload = \"\";\nfor (let i = 0; i < 11; i++) {\n    let sDatum = dDate10.toISOString().substring(0, 10);\n    //node.warn(\"Datum / 10TageWert \" + sDatum + \" / \" + oDaten.Bezug_Tag[sDatum]);\n    let fValue = 0;\n    if ( oDaten.Bezug_Tag[sDatum] ) {\n        fValue = oDaten.Bezug_Tag[sDatum];\n        aBarValues10.push(fValue.toFixed(2));\n    } else {\n        //node.warn(\"10Tage: \" + sDatum + \" => \" + fValue);\n        aBarValues10.push(0);\n    }\n    aBarLabels10.push(sDatum.substring(8, 10));\n    dDate10.setDate(dDate10.getDate() + 1);\n    if (i > 0) {\n        msg10Tage.Summe += fValue;\n    }\n}\nif (msg10Tage.Summe > 0 ) {\n    msg10Tage.Summe = msg10Tage.Summe.toFixed(1);\n} else {\n    msg10Tage.Summe = 0;\n}\naBarValues10.shift();\naBarLabels10.shift();\nmsg10Tage.aBarValues = aBarValues10;\nmsg10Tage.aBarLabels = aBarLabels10;\nmsg10Tage.payload = [{\n    \"labels\": aBarLabels10,\n    \"series\": [\"Verbrauch\"],\n    \"data\": [aBarValues10]\n}];\n\nreturn [ msg, msg10Tage ];\n*/",
