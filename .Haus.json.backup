        "name": "prepare",
        "func": "msg.Unit = global.get(\"StromUnit\") || \"kWh\";\nlet iTZOffset = global.get(\"TimezoneOffset\");\nlet dDatum = new Date(msg.payload[0][0].DATUM);\nmsg.Datum = dDatum.toLocaleString('de-de');\nmsg.Stromzaehler = (msg.payload[0][0].WERT/1000);\nmsg.topic = \"\";\nlet oUmrechnungen = msg.payload[1];\nmsg.o2Euro = oUmrechnungen.find(oItem => (oItem.TYP === \"Strom_kWh_€\") );\nlet i2Euro = msg.o2Euro ? msg.o2Euro.ParamZahl_1 : 0\nlet i2EuroGP = msg.o2Euro ? msg.o2Euro.ParamZahl_2 : 0\nmsg.i2Euro = i2Euro;\nmsg.i2EuroGP = i2EuroGP;\nlet msgJahre = {};\nmsgJahre.Unit = msg.Unit;\nmsgJahre.Summe = 0;\nlet msgMonate = {};\nmsgMonate.Unit = msg.Unit;\nmsgMonate.Summe = 0;\nlet msgTage = {};\nmsgTage.Unit = msg.Unit;\nmsgTage.Summe = 0;\nlet fLastValue = 0;\nlet oRange = { Jahr:  { len: 4, cnt: 6,  von: 0, bis: 4, oMsg: msgJahre }, \n               Monat: { len: 7, cnt: 13, von: 5, bis: 7, oMsg: msgMonate}, \n               Tag:   {len: 10, cnt: 10, von: 8, bis: 10,oMsg: msgTage  } };\nlet oKat = { Bezug: { show: true }, Auto: { show: true }, PV: { show: true }, Ein: { show: true }, BKW: { show: true }, Lad: { show: false }, Entlad: {show: false},\n             Haus:  { show:true }, Verbrauch: { show: true }, Direkt: { show: false } };\n\n/*\n0:select DATUM, WERT from SMA_STROM_BEZUG order by DATUM desc limit 1;\n1:select * from Parameter where TYP like 'Strom%' order by DATUM_VON desc;\n2: Bezug_Jahr  => SELECT SUBSTRING(DATUM, 1, 4) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4) order by SUBSTRING(DATUM, 1, 4) asc;\n3: Bezug_Monat => SELECT SUBSTRING(DATUM, 1, 7) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\n4: Bezug_Tag   => SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM, 1, 10) union(SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) as 'WERT' FROM STROM_BEZUG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1) order by DATUM asc;\n5: Auto_Jahr   => SELECT SUBSTRING(DATUM, 1, 4) as 'DATUM', MAX(Energie) as 'WERT' FROM ECAR where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4) order by SUBSTRING(DATUM, 1, 4) asc;\n6: Auto_Monat  => SELECT SUBSTRING(DATUM, 1, 7) as 'DATUM', MAX(ENERGIE) as 'WERT' FROM ECAR where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7) order by DATUM asc;\n7: Auto_Tag    => SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(ENERGIE) as 'WERT' FROM ECAR where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM, 1, 10) union(SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(ENERGIE) as 'WERT' FROM ECAR where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1) order by DATUM asc;\n8: PV_Jahr     => SELECT SUBSTRING(DATUM, 1, 4) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4) order by SUBSTRING(DATUM, 1, 4) asc;\n9: PV_Monat    => SELECT SUBSTRING(DATUM, 1, 7) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7) order by DATUM asc;\n10: PV_Tag     => SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM, 1, 10) union(SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) as 'WERT' FROM PV_GESAMT_ERTRAG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1) order by DATUM asc;\n11: Ein_Jahr   => SELECT SUBSTRING(DATUM, 1, 4) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\n12: Ein_Monat  => SELECT SUBSTRING(DATUM, 1, 7) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\n13: Ein_Tag    => SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM, 1, 10) union(SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) as 'WERT' FROM SMA_STROM_EINSPEISUNG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1) order by DATUM asc;\n14: BKW_Jahr   => SELECT SUBSTRING(DATUM, 1, 4) as 'DATUM', MAX(TotalEnergy) / 1000 as 'WERT' FROM BKL where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\n15: BKW_Monat  => SELECT SUBSTRING(DATUM, 1, 7) as 'DATUM', MAX(TotalEnergy) / 1000 as 'WERT' FROM BKL where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\n16: BKW_Tag    => SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(TotalEnergy) / 1000 as 'WERT' FROM BKL where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM, 1, 10) union(SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(TotalEnergy) as 'WERT' FROM BKL where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1) order by DATUM asc;\n17: Lad_Jahr   => SELECT SUBSTRING(DATUM, 1, 4) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM BATTERIE_LADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\n18: Lad_Monat  => SELECT SUBSTRING(DATUM, 1, 7) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM BATTERIE_LADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\n19: Lad_Tag    => SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM BATTERIE_LADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM, 1, 10) union(SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) as 'WERT' FROM BATTERIE_LADUNG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1) order by DATUM asc;\n20: Entlad_Jahr > SELECT SUBSTRING(DATUM, 1, 4) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 6 YEAR) group by SUBSTRING(DATUM, 1, 4);\n21: Entlad_Monat> SELECT SUBSTRING(DATUM, 1, 7) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 14 MONTH) group by SUBSTRING(DATUM, 1, 7);\n22: Entlad_Tag => SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) / 1000 as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM >= DATE_ADD(NOW(), INTERVAL - 11 DAY) group by SUBSTRING(DATUM, 1, 10) union(SELECT SUBSTRING(DATUM, 1, 10) as 'DATUM', MAX(WERT) as 'WERT' FROM BATTERIE_ENTLADUNG where DATUM < DATE_ADD(NOW(), INTERVAL - 11 DAY) LIMIT 1) order by DATUM asc;\n*/\n\nlet aMap = [ {idx:2,  Kat: \"Bezug\", Typ: \"Jahr\"}, {idx:3,  Kat: \"Bezug\", Typ: \"Monat\"}, {idx:4,  Kat: \"Bezug\", Typ: \"Tag\"}, {idx:5,  Kat: \"Auto\", Typ: \"Jahr\"}, {idx:6,  Kat: \"Auto\", Typ: \"Monat\"}, {idx:7,  Kat: \"Auto\", Typ: \"Tag\"}, \n             {idx:8,  Kat: \"PV\",    Typ: \"Jahr\"}, {idx:9,  Kat: \"PV\",    Typ: \"Monat\"}, {idx:10, Kat: \"PV\",    Typ: \"Tag\"}, {idx:11, Kat: \"Ein\",  Typ: \"Jahr\"}, {idx:12, Kat: \"Ein\",  Typ: \"Monat\"}, {idx:13, Kat: \"Ein\",  Typ: \"Tag\"},\n             {idx:14, Kat: \"BKW\",   Typ: \"Jahr\"}, {idx:15, Kat: \"BKW\",   Typ: \"Monat\"}, {idx:16, Kat: \"BKW\",   Typ: \"Tag\"}, {idx:17, Kat: \"Lad\",  Typ: \"Jahr\"}, {idx:18, Kat: \"Lad\",  Typ: \"Monat\"}, {idx:19, Kat: \"Lad\",  Typ: \"Tag\"},\n             {idx:20, Kat: \"Entlad\",Typ: \"Jahr\"}, {idx:21, Kat: \"Entlad\",Typ: \"Monat\"}, {idx:22, Kat: \"Entlad\",Typ: \"Tag\"}];\nlet aCalc = [ { Kat: \"Haus\",   Def: [ \"PV\", \"BKW\", \"Bezug\", \"Entlad\", \"-Auto\", \"-Lad\", \"-Ein\"]}, \n              { Kat: \"Direkt\", Def: [ \"PV\", \"BKW\", \"Entlad\", \"-Ein\", \"-Lad\"]},\n              { Kat: \"Verbrauch\", Def: [ \"Haus\", \"Auto\"]}];\nlet oDaten = {};\n//  add Data-Fields into oDaten\naMap.forEach( oItm => {\n    let idx = oItm.idx;\n    let sTyp = oItm.Typ;\n    let sKat = oItm.Kat;\n    if ( !oDaten[sTyp] ) {\n        oDaten[sTyp] = {};\n    }\n    if ( !oDaten[sTyp][sKat]) {\n        oDaten[sTyp][sKat] = {}\n    }\n    // node.warn(\" idx: \" + idx + \" sTyp: \" + sTyp + \" Kat: \" + sKat);\n    fLastValue = 0;\n    msg.payload[idx].forEach(oItem => {\n        let iLen = oRange[sTyp].len;\n        let sDate = oItem.DATUM.substring(0,iLen);\n        let fValue = parseInt(10 * (oItem.WERT - fLastValue)) / 10;\n        fLastValue = oItem.WERT;\n        if (!oDaten[sTyp][sKat][sDate]) {\n            if (msg.Unit === \"€\") {\n                fValue = fValue * i2Euro;\n            }\n            oDaten[sTyp][sKat][sDate] = fValue;\n        }\n    });\n});\n// add Cald-Fields into oDaten\nObject.keys(oRange).forEach( sRange => {\n    aCalc.forEach( oItm => {\n        let sKat = oItm.Kat;\n        let aDef = oItm.Def;\n        if ( !oDaten[sRange][sKat] ) {\n            oDaten[sRange][sKat] = {};\n        }\n        aDef.forEach( oSubKat => {\n            let sSubKat = oSubKat.substring(0,1) === \"-\" ? oSubKat.slice(1) : oSubKat; \n            let bSub = oSubKat.substring(0,1) === \"-\" ? true : false; \n            Object.keys(oDaten[sRange][sSubKat]).forEach( sDate => {\n                if ( !oDaten[sRange][sKat][sDate] ) {\n                    oDaten[sRange][sKat][sDate] = 0;\n                }\n                if (bSub) {\n                    oDaten[sRange][sKat][sDate] -= oDaten[sRange][sSubKat][sDate];\n                } else {\n                    oDaten[sRange][sKat][sDate] += oDaten[sRange][sSubKat][sDate];\n                }\n            })\n        });\n    });\n});\nmsg.oDaten = oDaten;\nmsg.Summe = 0;\nmsg.payload = \"\";\nlet oBarValues = {};\nlet oBarLabels = {};\nlet oBarSeries = {};\nlet oBarData =   {};\n// prepare chart structure\nObject.keys(oRange).forEach( sRange => {\n    let iLen = oRange[sRange].len;\n    let iCnt = oRange[sRange].cnt;\n    let iVon = oRange[sRange].von;\n    let iBis = oRange[sRange].bis;\n    let oMsg = oRange[sRange].oMsg;\n\n    let dDate = new Date();\n    dDate.setHours(dDate.getHours() + 6);\n\n    //node.warn(\"Range: \" + sRange + \" Cnt: \" + iCnt + \" Date: \" + dDate.toISOString().substring(0, 10) );\n\n    switch (sRange) {\n        case \"Jahr\":\n            dDate.setFullYear(dDate.getFullYear() - iCnt + 1);\n            break;\n        case \"Monat\":\n            dDate.setMonth(dDate.getMonth() - iCnt + 1);\n            break;\n        case \"Tag\":\n            dDate.setDate(dDate.getDate() - iCnt + 1);\n    }\n    //node.warn(\"Range: \" + sRange + \" Date: \" + dDate.toISOString().substring(0, 10));\n\n    for (let i = 1; i <= iCnt; i++) {\n        let sDate = dDate.toISOString().substring(0, iLen);\n        //node.warn(\"Range: \" + sRange + \" Date: \" + sDate + \" Datum: \" + dDate.toISOString().substring(0, 10));\n        Object.keys(oDaten[sRange]).forEach( sKat => {\n            if (!oBarValues[sRange]) {\n                oBarValues[sRange] = {};\n            }\n            if (!oBarValues[sRange][sKat]) {\n                oBarValues[sRange][sKat] = [];\n            }\n            if (!oBarData[sRange]) {\n                oBarData[sRange] = [];\n            }\n            if (!oBarLabels[sRange]) {\n                oBarLabels[sRange] = [];\n            }\n            if (!oBarSeries[sRange]) {\n                oBarSeries[sRange] = [];\n            }\n            let fValue = oDaten[sRange][sKat][sDate] ? oDaten[sRange][sKat][sDate] : 0;\n            oBarValues[sRange][sKat].push(fValue.toFixed(2));\n            if (i === iCnt && oKat[sKat].show ) {\n                oBarSeries[sRange].push(sKat);\n                oBarData[sRange].push(oBarValues[sRange][sKat]);\n            }\n        })\n        oBarLabels[sRange].push(sDate.substring(iVon, iBis));\n//        if ( sRange === \"Monat\" ) {\n//            node.warn(\"Range: \" + sRange + \" Date: \" + dDate.toISOString().substring(0,10) + \" Label: \" + sDate.substring(iVon, iBis));\n//        }\n        switch (sRange) {\n            case \"Jahr\":\n                dDate.setFullYear(dDate.getFullYear() + 1 );\n                break;\n            case \"Monat\":\n                dDate.setMonth(dDate.getMonth() + 1 );\n                break;\n            case \"Tag\":\n                dDate.setDate(dDate.getDate()   + 1 );\n        }\n    }\n\n    oMsg.payload = [{\n        \"labels\": oBarLabels[sRange],\n        \"series\": oBarSeries[sRange],\n        \"data\": oBarData[sRange]\n    }];\n});\n\nmsg.oBarValues = oBarValues;\nmsg.oBarLabels = oBarLabels;\nmsg.oBarSeries = oBarSeries;\nmsg.oBarData   = oBarData;\nreturn [msg, msgJahre, msgMonate, msgTage];",
